#loginwrap{
    height: 900px;
    background-color: #ffffff;
    background-image: url('../img/wuhan.png');
    background-size: 100% 100%;
}
#login{
    width: 1000px;
    height: 468px;
    border-radius: 3px;
    border: 1px solid #eaeaea;

}
#login>.container{height: 800px}
.logotitle{font-size: 32px}
.loginnav:after{
    content: "";
    width: 100%;
    height: 1px;
    position: absolute;
    bottom: 0px;
    background-color: #eeeeee;

}
.btn-warning {
    color: #212529;
    background: -webkit-linear-gradient(#ff9a07, #fb7418); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(#ff9a07, #fb7418); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(#ff9a07, #fb7418); /* Firefox 3.6 - 15 */
    background: linear-gradient(#ff9a07, #fb7418); /* 标准的语法 */
    border-color: #ffc107;
}
.btn.focus, .btn:focus {
    outline: 0;
    box-shadow: 0 0 0 0.0rem rgba(0,123,255,.25);
}
.form-control:focus {
    color: #495057;
    background-color: #fff;
    border-color: #80bdff;
    outline: 0;
    box-shadow: 0 0 0 0.0rem rgba(0,123,255,.25);
}
.text-secondary{color:#eeeeee !important; }
.inputborder{border: 2px solid #eeeeee !important;}
.bg-warning{background-color: #f08c00 !important}
.help-block{color: red}
