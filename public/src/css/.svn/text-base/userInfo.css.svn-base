.margin_left10{margin-left:10px}
.margin_top10{margin-top:10px}
.borderGray{border:1px solid #dee2e6}
.numTip{padding:0 3px;color:#fff;border-radius:5px;position:absolute;bottom: -2px;right:-10px;background:rgba(255,89,56,0.8) }
.btnTip{background: #f08c00;padding:4px 10px;border-radius: 5px 0 0 5px;color:#fff!important;position: absolute;top:20px;right:0}
.ant-tabs-bar.ant-tabs-left-bar{position: absolute;left:15px;bottom:0px!important;width:300px;height:500px!important;border:1px solid #dee2e6!important;box-shadow: 0 .5rem 1rem rgba(0,0,0,.15)!important }
.ant-tabs .ant-tabs-left-bar .ant-tabs-tab{text-align: center!important;padding:24px!important;}
.ant-tabs-nav .ant-tabs-tab-active{border:none!important;position: relative}
.ant-tabs-nav .ant-tabs-tab-active:after{content: "";position: absolute;bottom:10px;left:115px;background:#f08c00;width:70px;height:2px;font-size: 16px!important;}
.ant-tabs-ink-bar.ant-tabs-ink-bar-animated{display: none!important;}
.ant-tabs .ant-tabs-left-content{padding-left:0!important;}
.ant-tabs .ant-tabs-left-bar .ant-tabs-tab{margin:0 0 10px 0!important;}
.ant-tabs .ant-tabs-left-bar .ant-tabs-tab:first-child{margin-top:10px!important;}
.tableBtn{padding:2px 10px;color:#fff;border-radius: 5px;background: #f08c00}
.tableBtn.delete{background: #ff5938}
.ant-table-pagination.ant-pagination{float:none!important;text-align: center}
.textRight{text-align: right}
.borderNone{border:none!important;}
.bg-gray{background: #f8f8f8}
